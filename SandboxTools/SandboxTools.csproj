<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyTitle>Sandbox Tools</AssemblyTitle>
    <FileVersion>3.2.0.0</FileVersion>
    <RootNamespace>PeterHan.SandboxTools</RootNamespace>
    <Description>Improves the Sandbox Mode tools with numerous small tweaks.</Description>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <Platforms>Vanilla;Spaced Out</Platforms>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'Vanilla' ">
    <ArchivedVersionPath>\archived_versions\vanilla</ArchivedVersionPath>
    <SupportedContent>VANILLA_ID</SupportedContent>
    <LastWorkingBuild>469300</LastWorkingBuild>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Platform)' == 'Spaced Out' ">
    <SupportedContent>VANILLA_ID, EXPANSION1_ID</SupportedContent>
    <LastWorkingBuild>469369</LastWorkingBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Destroy.png" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Destroy.png" />
  </ItemGroup>
</Project>
